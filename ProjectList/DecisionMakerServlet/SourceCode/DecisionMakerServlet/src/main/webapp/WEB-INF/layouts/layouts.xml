<tiles-definitions>
  <definition name="default" template="/WEB-INF/layouts/default.jsp">
    <put-attribute name="header" value="/WEB-INF/views/header.jsp" />
    <put-attribute name="menu" value="/WEB-INF/views/menu.jsp" />
    <put-attribute name="footer" value="/WEB-INF/views/footer.jsp" />
  </definition>
  <definition name="loginlayout" template="/WEB-INF/layouts/loginlayout.jsp">
    <put-attribute name="login-header" value="/WEB-INF/views/Login/login-header.jsp" />
    <put-attribute name="login-body-left" value="/WEB-INF/views/Login/login-body-left.jsp" />
    <put-attribute name="login-body-right" value="/WEB-INF/views/Login/login-body-right.jsp" />
    <put-attribute name="login-footer" value="/WEB-INF/views/Login/login-footer.jsp" />
  </definition>
  <definition name="changepwdlayout" template="/WEB-INF/layouts/changepwdlayout.jsp">
  </definition>
  
  <definition name="viewAnnouncement" template="/WEB-INF/views/Announcement/viewContentAnnouncement.jsp">
  </definition>
  <definition name="viewRule" template="/WEB-INF/views/Rule/viewContentRule.jsp">
  </definition>
